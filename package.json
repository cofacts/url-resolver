{
  "name": "link-resolver",
  "version": "1.0.0",
  "description": "Scraps URL and returns summarization results",
  "main": "index.js",
  "scripts": {
    "start": "npm run compile && node index.js",
    "lint": "eslint index.js docker-test.js src/",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "compile": "grpc_tools_node_protoc --proto_path=src/typeDefs --js_out=import_style=commonjs,binary:src/lib src/typeDefs/resolve_error.proto"
  },
  "author": "MrOrz",
  "license": "MIT",
  "dependencies": {
    "@grpc/proto-loader": "^0.1.0",
    "dotenv": "^6.0.0",
    "google-protobuf": "^3.9.1",
    "grpc": "^1.11.0",
    "grpc-tools": "^1.8.0",
    "node-fetch": "^2.2.0",
    "puppeteer": "^2.1.0",
    "rollbar": "^2.5.1",
    "unfurl.js": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "jest": {
    "clearMocks": true
  }
}
